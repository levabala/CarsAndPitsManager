<Window xmlns:Visualizers="clr-namespace:CarsAndPitsWPF2.Classes.Visualizers"  x:Class="CarsAndPitsWPF2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Name="MyWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid x:Name="MainGrid">
        <Border x:Name="BorderContainer" BorderBrush="Black" BorderThickness="1" Background="#f2f2f2"/>
        <Visualizers:CPNetVisualizerGMap x:Name="MyVisualizer"/>
        <ProgressBar x:Name="LoadProgressBar" Minimum="0" Maximum="100" HorizontalAlignment="Left" Height="10" VerticalAlignment="Top" Width="100" Margin="408,10,0,0"/>
    </Grid>
</Window>
